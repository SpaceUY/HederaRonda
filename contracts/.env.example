# Deployer's private key
PRIVATE_KEY=your_private_key_here

# RPC URLs for different networks
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_PROJECT_ID
MAINNET_RPC_URL=https://mainnet.infura.io/v3/YOUR_PROJECT_ID

# Chainlink VRF Parameters Sepolia
VRF_SUBSCRIPTION_ID=1  # Replace with your subscription ID
VRF_COORDINATOR=0x9DdfaCa8183c41ad55329BdeeD9F6A8d53168B1B  # Replace with actual VRF coordinator address
VRF_KEY_HASH=0x787d74caea10b2b357790d5b5247c2f63d1d91572a9846f780606e4d953677ae  # Replace with your key hash
VRF_CALLBACK_GAS_LIMIT=40000

# After factory deployment, add these variables
RONDA_FACTORY_ADDRESS=0x0000000000000000000000000000000000000000  # Replace with deployed factory address
PENALTY_TOKEN_ADDRESS=0x0000000000000000000000000000000000000000  # Replace with deployed penalty token address
PAYMENT_TOKEN_ADDRESS=0x0000000000000000000000000000000000000000  # Replace with your ERC20 token address

# CCIP environment variables
CCIP_ROUTER=0x0000000000000000000000000000000000000000

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here